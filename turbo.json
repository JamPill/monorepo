{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", ".open-next/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "generate:types": {
      "cache": false
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
